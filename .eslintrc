{
 "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "global-require": 0,
    "import/no-unresolved": 0,
    "no-console": 0,
    "react/jsx-closing-bracket-location": 0,
    "newline-per-chained-call": 0,
    "react/prefer-stateless-function": 0,
    "max-len": 0,
    "eslint-disable-line global-require": 0
  },
  "globals": {
    "__DEV__": true
  }
}
